snippet remote_state
  remote_state {
    backend = "${1:backend}"

    config = {
      ${2}
    }

    generate = {
      path = "${3:path}"
      if_exists = "${4:overwrite_terragrunt}"
    }
  }

snippet generate
  generate "${1:name}" {
    path = "${2:path}"
    if_exists = "${3:overwrite_terragrunt}"
    contents = <<EOF
  ${4}
  EOF
  }
